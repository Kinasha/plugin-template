{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["ES6"],
    "strict": true,
    "useDefineForClassFields": true,
    "typeRoots": ["./node_modules/@types", "./node_modules/@figma"],
    "allowJs": false,
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "sourceMap": false,
    "importHelpers": true,
    "resolveJsonModule": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "outDir": "../dist",
    "noEmit": true
  }
}
